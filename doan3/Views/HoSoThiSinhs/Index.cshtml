@model IEnumerable<doan3.Models.HoSoThiSinh>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ImgThisinh)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoaiHoso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ngaydk)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Khamsuckhoe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ghichu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Hang)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Hocvien)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ImgThisinh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoaiHoso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ngaydk)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Khamsuckhoe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ghichu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Hang.HangId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Hocvien.HocvienId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.HosoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.HosoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.HosoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
