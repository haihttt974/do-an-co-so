@model User
<form asp-action="Register" method="post">
    <input asp-for="Username" placeholder="Username" required />
    <input asp-for="Email" placeholder="Email" required />
    <input asp-for="Sdt" placeholder="SĐT" required />
    <input asp-for="Diachi" placeholder="Địa chỉ" />
    <input asp-for="Password" type="password" placeholder="Mật khẩu" required />
    <button type="submit">Đăng ký</button>
</form>
